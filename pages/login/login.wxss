.container {
  height: 100vh;
  display: flex;
  flex-direction: column;
  padding: 30rpx;
}

text {
  font-size: 40rpx;
  display: flex;
  justify-content: center;
}

input {
  border: 2rpx solid #336699;
  height: 90rpx;
  border-radius: 12rpx;
  padding-left: 16rpx;
}

.phone_number {
  margin: 16rpx 0;
}

.code {
  display: flex;
  flex-direction: row;
  width: 100%;
  margin-bottom: 16rpx;
}